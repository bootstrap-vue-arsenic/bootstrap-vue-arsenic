{
  "name": "bootstrap-vue-arsenic",
  "description": "The Bootstrap Vue admin components.",
  "version": "0.1.0",
  "keywords": [
    "css",
    "sass",
    "bootstrap",
    "vue",
    "admin",
    "components"
  ],
  "main": "dist/bootstrap-vue-arsenic.common.js",
  "web": "dist/bootstrap-vue-arsenic.js",
  "module": "es/index.js",
  "style": "dist/bootstrap-vue-arsenic.css",
  "author": "Lucas Yang",
  "license": "MIT",
  "scripts": {
    "build": "sh scripts/build.sh",
    "watch": "npm-run-all --parallel watch-*",
    "watch-css": "nodemon --watch src/ --ext scss --exec \"sh scripts/compile-sass.sh\"",
    "watch-js": "nodemon --watch src/ --ext js --exec \"sh scripts/compile-js.sh\"",
    "test": "jest",
    "lint": "eslint --ext .js,.vue ./",
    "prettify": "prettier --write \"**/*.{js,json,md,scss,vue}\"",
    "dist": "npm-run-all build test",
    "release": "standard-version"
  },
  "homepage": "https://github.com/ycs77/bootstrap-vue-arsenic",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/bootstrap-vue-arsenic.git"
  },
  "bugs": {
    "url": "https://github.com/ycs77/bootstrap-vue-arsenic/issues"
  },
  "dependencies": {
    "bootstrap": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "autoprefixer": "^9.4.8",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "clean-css-cli": "^4.2.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "esm": "^3.2.6",
    "jest": "^24.1.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "prettier": "^1.16.4",
    "rollup": "^1.2.5",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-vue": "^4.7.2",
    "shx": "^0.3.2",
    "standard-version": "^5.0.0",
    "terser": "^3.16.1",
    "vue": "^2.6.7",
    "vue-template-compiler": "^2.6.7"
  },
  "jest": {
    "testRegex": "spec.js$",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "coverageDirectory": "./coverage/"
  }
}
