{
  "name": "bootstrap-vue-arsenic",
  "description": "The bootstrap vue admin components.",
  "version": "0.2.0",
  "keywords": [
    "css",
    "sass",
    "bootstrap",
    "vue",
    "admin",
    "components"
  ],
  "main": "dist/bootstrap-vue-arsenic.common.js",
  "web": "dist/bootstrap-vue-arsenic.js",
  "module": "es/index.js",
  "style": "dist/bootstrap-vue-arsenic.css",
  "author": "Lucas Yang",
  "license": "MIT",
  "scripts": {
    "css": "npm-run-all css-compile css-prefix css-minify css-copy",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 src/index.scss dist/bootstrap-vue-arsenic.css && npm run css-copy",
    "css-prefix": "postcss --config scripts/postcss.config.js --replace dist/bootstrap-vue-arsenic.css",
    "css-minify": "cleancss --level 1 --format breaksWith=lf --source-map --source-map-inline-sources --output dist/bootstrap-vue-arsenic.min.css dist/bootstrap-vue-arsenic.css",
    "css-copy": "cross-env-shell shx mkdir -p docs/css/ && cross-env-shell shx cp dist/*.css docs/css/",
    "js": "npm-run-all check-plugin-meta js-compile js-es js-minify js-copy",
    "js-compile": "rollup -c scripts/rollup.config.js && npm run js-copy",
    "js-es": "npm-run-all js-es-clean js-es-build",
    "js-es-clean": "cross-env-shell shx rm -rf es",
    "js-es-build": "cross-env NODE_ENV=es babel src --out-dir es --ignore 'src/**/*/fixtures,src/**/*.spec.js'",
    "js-minify": "npm-run-all --parallel js-minify-*",
    "js-minify-original": "terser dist/bootstrap-vue-arsenic.js --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/bootstrap-vue-arsenic.js.map,includeSources,url=bootstrap-vue-arsenic.min.js.map\" --output dist/bootstrap-vue-arsenic.min.js",
    "js-minify-common": "terser dist/bootstrap-vue-arsenic.common.js --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/bootstrap-vue-arsenic.common.js.map,includeSources,url=bootstrap-vue-arsenic.common.min.js.map\" --output dist/bootstrap-vue-arsenic.common.min.js",
    "js-minify-esm": "terser dist/bootstrap-vue-arsenic.esm.js --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/bootstrap-vue-arsenic.esm.js.map,includeSources,url=bootstrap-vue-arsenic.esm.min.js.map\" --output dist/bootstrap-vue-arsenic.esm.min.js",
    "js-copy": "cross-env-shell shx mkdir -p docs/js/ && cross-env-shell shx cp dist/*.js docs/js/",
    "docs-serve": "serve -n",
    "watch": "npm-run-all --parallel watch-*",
    "watch-css": "nodemon --watch src/ --ext scss --exec \"npm run css-compile\"",
    "watch-js": "nodemon --watch src/ --ext js,vue --exec \"npm run js-compile\"",
    "check-plugin-meta": "node -r esm scripts/check-plugin-meta.js",
    "build": "npm-run-all prettify lint css js",
    "lint": "eslint --ext .js,.vue ./",
    "prettify": "prettier --write \"**/*.{js,json,md,scss,vue}\"",
    "release": "npm run build && standard-version"
  },
  "homepage": "https://github.com/ycs77/bootstrap-vue-arsenic",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/bootstrap-vue-arsenic.git"
  },
  "bugs": {
    "url": "https://github.com/ycs77/bootstrap-vue-arsenic/issues"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "lodash": "^4.17.11",
    "serve": "^10.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "autoprefixer": "^9.4.8",
    "babel-eslint": "^10.0.1",
    "clean-css-cli": "^4.2.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "esm": "^3.2.6",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "prettier": "^1.16.4",
    "rollup": "^1.2.5",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-vue": "^4.7.2",
    "shx": "^0.3.2",
    "standard-version": "^5.0.0",
    "terser": "^3.16.1",
    "vue": "^2.6.7",
    "vue-template-compiler": "^2.6.7"
  }
}
