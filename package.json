{
  "name": "bootstrap-vue-arsenic",
  "description": "The Bootstrap Vue admin components.",
  "version": "0.1.0",
  "keywords": [
    "css",
    "sass",
    "bootstrap",
    "vue",
    "admin",
    "components"
  ],
  "main": "dist/bootstrap-vue-arsenic.common.js",
  "web": "dist/bootstrap-vue-arsenic.js",
  "module": "es/index.js",
  "style": "dist/bootstrap-vue-arsenic.css",
  "author": "Lucas Yang",
  "license": "MIT",
  "scripts": {
    "build": "sh scripts/build.sh",
    "watch": "npm-run-all --parallel watch-*",
    "watch-css": "nodemon --watch src/ --ext scss --exec \"sh scripts/compile-sass.sh\"",
    "watch-js": "nodemon --watch src/ --ext js --exec \"sh scripts/compile-js.sh\"",
    "docs-dev": "cross-env NODE_ENV=docs nuxt dev -c docs/nuxt.config.js",
    "docs-build": "cross-env NODE_ENV=docs DEPLOY_ENV=gh_pages nuxt build -c docs/nuxt.config.js",
    "docs-gen": "cross-env NODE_ENV=docs DEPLOY_ENV=gh_pages nuxt generate -c docs/nuxt.config.js",
    "docs-start": "cross-env NODE_ENV=docs DEPLOY_ENV=gh_pages nuxt start -c docs/nuxt.config.js",
    "docs-publish": "npm run docs-gen && gh-pages -t -d docs-dist",
    "test": "jest",
    "lint": "eslint --ext .js,.md,.vue ./",
    "prettify": "prettier --write \"**/*.{js,json,md,scss,vue}\"",
    "dist": "npm-run-all prettify test build",
    "release": "standard-version"
  },
  "homepage": "https://ycs77.github.io/bootstrap-vue-arsenic/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/bootstrap-vue-arsenic.git"
  },
  "bugs": {
    "url": "https://github.com/ycs77/bootstrap-vue-arsenic/issues"
  },
  "files": [
    "src",
    "dist",
    "nuxt",
    "es"
  ],
  "sideEffects": [
    "./es/utils/object.js",
    "./src/utils/object.js",
    "./es/utils/array.js",
    "./src/utils/array.js",
    "**/*.scss",
    "**/*.css",
    "**/*.vue",
    "./docs"
  ],
  "dependencies": {
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.13",
    "vue-functional-data-merge": "^2.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/standalone": "^7.4.3",
    "@nuxtjs/pwa": "^3.0.0-beta.12",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.5.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.0",
    "clean-css-cli": "^4.2.1",
    "codemirror": "^5.45.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "esm": "^3.2.22",
    "gh-pages": "^2.0.1",
    "highlightjs": "^9.12.0",
    "html-loader": "^0.5.5",
    "jest": "^24.7.0",
    "lodash": "^4.17.11",
    "marked": "^0.6.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "nuxt": "^2.5.1",
    "postcss-cli": "^6.1.2",
    "prettier": "^1.16.4",
    "rollup": "^1.8.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-vue": "^4.7.2",
    "sass-loader": "^7.1.0",
    "shx": "^0.3.2",
    "standard-version": "^5.0.2",
    "terser": "^3.17.0",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.6.10"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "./node_modules/.bin/prettier --write CHANGELOG.md"
    }
  }
}
